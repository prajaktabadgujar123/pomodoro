{
  "manifest_version": 3,
  "name": "Pomodoro",
  "description": "Demo Chrome Extension that uses React and Webpack",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
